  "description": "Discord bot to manager cryptocurrencies in binance exchange",
  "keywords": [
    "bot",
    "typescript",
    "discord-js",
    "cryptocurrence",
    "bitcoin",
    "binance",
    "exchange"
  ],
  "main": "dist/src/index.js",
  "author": {
    "name": "Edixon Alberto",
    "email": "edixonalberto@gmail.com"
  },
  "license": "MIT",
